@model Categorie

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">@Html.DisplayNameForInnerType((Categorie model) => model.Nom)</th>
      <th scope="col">@Html.DisplayNameForInnerType((Categorie model) => model.ProduitList)</th>
    </tr>
  </thead>
  <tbody>
       <tr>
            @Model.Id
      </tr>
      <tr>
          @Model.Nom
      </tr>
        @foreach (var p in Model.ProduitList)
        {
            <tr>
                <td>@p.Nom</td>
@*                 <td>@p.Description</td>
                <td>@p.Prix</td>
                <td>@p.Quantite</td>
                <td>@p.Categorie</td> *@
            </tr> 
        }
  </tbody>
</table>